      <div class="page">
        <div class="top-nav">
          <div class="container-fluid">
            <p class="t-subtitle-1 pages-tree pages-tree-active">
              Tableau de bord
            </p>
            <a href="#">CrédApp DCR</a>
          </div>
        </div>
        <div class="main">
          <div class="container-fluid">
            <div class="row update-row">

            <div class="row main-row">
              <div class="main-files-col col-8">
                <div class="paper">
                  <h5>Nouveaux Dossiers</h5>
                  <hr />
                  <table class="table-demandes">
                    <thead>
                      <tr>
                        <th scope="col">ID Crédit</th>
                        <th scope="col">Capital</th>
                        <th scope="col">Le durée de crédit</th>
                        <th scope="col">La monsualité</th>

                      </tr>
                    </thead>
                    <tbody>
                      <% items.forEach(function(item,index){ %>
                        <tr>
                          <td><%= item.idCrédit %></td>
                          <td><%= item.capital %></td>
                          <td><%= item.duréeCr %></td>
                          <td><%= item.monsualité %></td>
                          
                          <td> <a href="/verifieDemandeDCR?id=<%= item.idCrédit %>" class="btn btn-primary">Vérifié</a> </td>  
        
                        </tr>
                        <% }); %>
                      
                    </tbody>
                  </table>
                </div>
              </div>
