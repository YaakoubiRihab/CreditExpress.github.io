<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>CrédApp</title>
    <link rel="stylesheet" href="/css/bootstrap4/bootstrap.min.css">
    <link rel="stylesheet" href="/fas/css/all.css">
    <link rel="stylesheet" href="/css/style.css">
</head>



<body>
<!-- Navbar -->
<nav class="navbar navbar-expand-md navbar-light fixed-top" id="navBar">
  <div class="container">
      <a class="navbar-brand" href="/profile"><img src="/img/logo.svg"></a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
          aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <ul class="navbar-nav ml-auto">
              <li class="nav-item">
                  <a class="" href="/profile">Accueil</a>
              </li>
              <li class="nav-item">
                  <a class="" href="/simulationClient">Simulation</a>
              </li>
              <li class="nav-item">
                  <a class="" href="/assistanceClient">Assistance</a>
              </li>
              <li class="nav-item">
                  <a class="" href="/contact">Contact</a>
              </li>
              <li class="nav-item">
                  <a class="btn-nav" href="/logout">Déconnexion</a>
              </li>
          </ul>
      </div>
  </div>
</nav>

<main class="p-pro">
<div class="container">
    <div class="row">
        <!-- Information utilisateur -->
        <div class="col-12 col-md-8 col-lg-9">
            <div class="paper pro-user">
                <h4><%= nom  %>&nbsp;&nbsp;<%= prenom %></h4>
                <ul class="fa-ul">
                <li>
                    <span class="fa-li"><i class="fas fa-map-marker-alt"></i></span><%= ville %>
                </li>
                <li>
                    <span class="fa-li"><i class="fa fa-credit-card inp-icon"></i></span><%= cin %>
                </li>
                <li>
                    <span class="fa-li"><i class="fa fa-envelope inp-icon"></i></span><%= email %>
                </li>
                </ul>
            </div>
        </div>
        <!-- nombre RDV -->
        <div class="d-none d-md-block col-md-4 col-lg-3">
          <div class="paper pro-ntf" id="paper-bg-green">
            <div class="nb-demandes text-center">
              <p class="t-body-2">Vous avez envoyez</p>
              <h3> <%= total %></h3>
              <p>Demandes</p>
            </div>
          </div>
        </div>
    </div>

    <div class="row">
        <!-- Menu -->
        <div class="col-12 col-md-4">
            <div class="paper pro-mnu" id="paper-bg-green">
              <ul>
                <li>
                  <a href="/config/donneesDemande" class="active">
                    <h6>
                      Mes demandes
                    </h6>
                  </a>
                </li>
              <!-- <li>
                  <a href="rendezvous.htm">
                    <h6>
                      Mes rendez-vous
                    </h6>
                  </a>
                </li>-->
                <li>
                  <a href="/config">
                    <h6>
                      Paramètres du compte
                    </h6>
                  </a>
                </li>
                <li>
                  <a href="/config/mdp">
                    <h6>
                      Changer le mot de passe
                    </h6>
                  </a>
                </li>
              </ul>
            </div>
        </div>
        <!-- Partie demande -->
        <div class="col-12 col-md-8">
            <div class="paper pro-main">
              <h5>Historique des demandes</h5>
              <hr />
                            <div class="profil-table-demande">
                  <!-- numero des demande -->
                <table-demande  num-demandes="<%= total %>" >
                    <!-- div de chaque demande -->
                    <% demandes[0].forEach(function(demande,index){ %>
                      
                      <div class="demande-wrapper" capital="<%= demande.capital %> Dt" mensualite="<%= demande.monsualité %> Dt" durre="<%= demande.duréeCr %>" etat="<%= demande.etat_dossier %>"></div>
                      
                      <% }); %>
                  </table-demande>
             

              </div>
            </div>
          </div>

    </div>

</div>
</main>



<page-footer num-service-client="70 000 050" apple="" google="" facebook="https://www.facebook.com"
instagram="https://www.instagram.com" youtube="https://www.youtube.com" linkedin="https://www.linkedin.com">
</page-footer>

<script src="/js/jquery-3.5.1.min.js"></script>
<script src="/js/bootstrap4/bootstrap.bundle.js"></script>
<script src="/js/bootstrap4/bootstrap.bundle.min.js"></script>

<script src="/js/comps/navbaroffline.js"></script>
<script src="/js/comps/navbaronline.js"></script>
<script src="/js/comps/tableDemandes.js"></script>
<script src="/js/comps/inputs.js"></script>
<script src="/js/comps/footer.js"></script>

<script src="/js/config.js"></script>
</body>

</html>